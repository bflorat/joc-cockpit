.embed-resp {
  position: relative;
  display: block;
  height: 0;
  padding: 0;
  overflow: hidden;
}
.embed-resp .embed-responsive-item,
.embed-resp iframe,
.embed-resp embed,
.embed-resp object,
.embed-resp video
{
  position: absolute;
  top: 0;
  bottom: 0;
  left: 0;
  width: 100%;
  height: 400px;
  border: 0;
}
.embed-resp-16by9 {
  padding-bottom: 40%;
}

